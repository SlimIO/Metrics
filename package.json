{
  "name": "@slimio/metrics",
  "version": "0.2.1",
  "description": "SlimIO Metrics",
  "main": "index.js",
  "engines": {
    "node": ">=8"
  },
  "private": false,
  "scripts": {
    "prepublishOnly": "pkg-ok",
    "test": "ava --verbose",
    "doc": "jsdoc -c ./jsdoc.json -r -R ./README.md -P ./package.json --verbose",
    "coverage": "nyc ava",
    "report": "nyc report --reporter=html"
  },
  "husky": {
    "hooks": {
      "pre-push": "cross-env npm test && eslint index.js",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SlimIO/Metrics.git"
  },
  "keywords": [
    "SlimIO",
    "monitoring"
  ],
  "author": "SlimIO",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SlimIO/Metrics/issues"
  },
  "homepage": "https://github.com/SlimIO/Metrics#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@escommunity/minami": "^1.0.0",
    "@slimio/addon": "^0.15.3",
    "@slimio/eslint-config": "^2.0.3",
    "@types/es6-shim": "^0.31.39",
    "@types/node": "^10.12.26",
    "ava": "^1.2.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.13.0",
    "husky": "^1.3.1",
    "jsdoc": "^3.5.5",
    "nyc": "^13.2.0",
    "pkg-ok": "^2.3.1"
  },
  "dependencies": {
    "@slimio/is": "^1.4.0"
  }
}
